<h1>Courses#index</h1>
<p>Find me in app/views/courses/index.html.erb</p>

<%= link_to "New Course", new_course_path %>

<%@courses.each do |course| %>
  <hr>
  <div>
    <%= course.title %>
    <%= course.level %>
    <%= button_to "Delete", course_path(course), method: :delete %>
    <%= button_to "Show", course_path(course), method: :get %>
  </div>
  <hr>
<% end %>